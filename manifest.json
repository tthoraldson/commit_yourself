
{
  "manifest_version": 2,

  "name": "Commit Yourself",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "0.0.1",

  "browser_action": {
    "default_icon": "img/icon64.png",
    "default_popup": "popup.html"
  },
  "icons": { "16": "img/icon64.png",
          "128": "img/icon128.png" },

 "permissions": [
    "activeTab"
 ]
}
